<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<property name="defaultPattern" value="%d{HH:mm:ss.SSS} [%-5level] %msg%n" />
	
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>${defaultPattern:-}</pattern>
		</encoder>
	</appender>
	
	<root level="WARN">
		<appender-ref ref="STDOUT" />
	</root>
	
	<logger name="tp.pdc.proxy" level="INFO" additivity="false">
		<appender-ref ref="STDOUT" />
	</logger>
</configuration>